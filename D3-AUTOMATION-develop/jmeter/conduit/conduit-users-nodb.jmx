          <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="End User &lt;/usr&gt;" enabled="true">
            <stringProp name="BeanShellSampler.query">
import java.io.BufferedWriter;
import java.io.File;
import java.io.FileWriter;


String xml = vars.get(&quot;xml&quot;);
xml += &quot;    &lt;/usr&gt;\n\n&quot;;

File file = new File(vars.get(&quot;filePath&quot;));
FileWriter fw = new FileWriter(file, true);
BufferedWriter bw = new BufferedWriter(fw);

bw.write(xml);
bw.flush();
bw.close();
</stringProp>
            <stringProp name="BeanShellSampler.filename"></stringProp>
