
# This is the config file required for the load generation script

# test settings
threadCount=100
loopCount=200
thinkTimeInMillis=200
thinkRangeInMillis=100
rampUp=5
serverBaseName=perf-synovusperf.d3vcloud.com
serverPort=443
serverProtocol=https

# number of users from the conduit process
numberOfAccounts=100000

# number of groups from the conduit account-create process
numOfGroups=2

# The starting point for account numbers
accountOffset=1001

# Starting point for group number
groupOffset=100

# Prefix for all accounts
accountPrefix=DDA-1-

# Prefix for user login id used in conduit process
userLoginPrefix=perf-

